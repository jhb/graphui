<span tal:omit-tag="" metal:define-macro="properties">
    <h5>Properties</h5>
    <span class="uk-description-list">
        <span tal:repeat="(key,value) sorted(obj.items())" tal:omit-tag="">
            <dt>${key}</dt>
            <dd tal:condition="type(value)==list">
                <ul class="uk-list uk-list-hyphen">
                    <li tal:repeat="entry value">${entry}</li>
                </ul>
            </dd>
            <dd tal:condition="type(value)!=list">${value}</dd>
        </span>
    </span>
</span>